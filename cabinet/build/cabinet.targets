<Project>
	<UsingTask TaskName="cabinet.Cabinet" AssemblyFile="$(MSBuildThisFileDirectory)../bin/Debug/netstandard2.0/cabinet.dll" />

	<Target Name="CabinetGenerate" AfterTargets="Build" Condition="'$(GenerateCabinet)' == 'true'">
		<Cabinet OutDir="$(OutDir)" TargetPath="$(TargetPath)" />
	</Target>
</Project>